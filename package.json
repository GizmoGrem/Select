{
  "name": "Select",
  "version": "1.0.0",
  "description": "Webpack 3 React, Redux and Stylus boilerplate",
  "private": true,
  "scripts": {
    "start": "clear && webpack-dev-server",
    "clean": "rimraf build",
    "lint": "eslint src || true",
    "build": "npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "deploy": "npm run build && cp now.json ./build && now ./build --public --static",
    "alias": "cp now.json ./build && cd ./build && now alias",
    "deploy-production": "npm run build && cp now.production.json ./build/now.json && now ./build --public --static",
    "alias-production": "cp now.production.json ./build/now.json && cd ./build && now alias"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "react",
    "webpack3",
    "boilerplate",
    "redux"
  ],
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "lodash": "^4.17.4",
    "open-browser-webpack-plugin": "0.0.5",
    "prettier": "1.11.1",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^0.5.9",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "autoprefixer-stylus": "^0.14.0",
    "axios": "^0.17.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "dotenv-webpack": "^1.5.4",
    "escape-string-regexp": "^1.0.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "happypack": "^4.0.0",
    "html-webpack-plugin": "^2.30.1",
    "img": "^3.0.3",
    "libs": "^0.1.2",
    "moment": "^2.19.2",
    "node-static": "^0.7.10",
    "prop-types": "^15.6.1",
    "public": "^0.1.2",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-click-outside": "^3.0.1",
    "react-collapse": "^4.0.3",
    "react-collapsible": "^2.0.4",
    "react-device-detect": "^0.1.0",
    "react-dom": "^16.1.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.1.2",
    "react-moment": "^0.6.8",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scroll": "^1.7.7",
    "react-slick": "^0.16.0",
